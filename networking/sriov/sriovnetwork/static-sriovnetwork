apiVersion: sriovnetwork.openshift.io/v1
kind: SriovNetwork
metadata:
  name: static-sriovnetwork
  namespace: openshift-sriov-network-operator
spec:
  ipam: |
    {
      "type": "static"
    }
  capabilities: |
    {
      "mac": true,
      "ips": true
    }
  vlan: 0
  maxTxRate: 100
  spoofChk: "on"
  trust: "off"
  resourceName: intelnetdevice
  networkNamespace: z2

